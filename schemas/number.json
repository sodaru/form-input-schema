{
  "$schema": "https://form-input-schema.json-schema.sodaru.com/meta-schemas/input.json",
  "$id": "https://form-input-schema.json-schema.sodaru.com/schemas/number.json",
  "type": "object",
  "additionalProperties": false,
  "required": ["type", "name"],
  "properties": {
    "type": { "const": "number" },
    "name": { "type": "string" },
    "label": { "type": "string" },
    "default": { "type": "number" },
    "minimum": { "type": "number" },
    "maximum": { "type": "number" },
    "multipleOf": { "type": "number" }
  }
}
